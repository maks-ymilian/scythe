desc [
	description: "square gfx test",

	in_pins: [],
	out_pins: [],

	options: [ gfx: [ hz: 60, ], ],
];

@gfx
{
	gfx.set(0.3, 0.5, 0.6);

	int spacing = 10;
	int count = 5;
	int size = math.max(gfx.mouse_x, gfx.mouse_y) / count;
    for (int x = 0; x < size * count; x += size)
		for (int y = 0; y < size * count; y += size)
			gfx.rect(x + spacing / 2, y + spacing / 2, size - spacing / 2, size - spacing / 2);
}
